                                        <tbody ng-repeat="(key, value) in caseStudyVm.cookieNoPlotValues">
                                        <tr>
                                            <td><span>{{key}}</span></td>
                                            <td><span>{{value}}</span></td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
        </section>

